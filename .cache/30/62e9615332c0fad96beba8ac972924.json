age: linear-gradient($fdc-teal-2, $fdc-teal-2), url($background-image);\n    background-size: cover;\n    background-repeat: no-repeat;\n    background-position: top right;\n    padding: 2rem 0;\n    background-color: $fdc-teal-2;\n    flex: 1;\n    width: 100%;\n\n    & > header {\n      background-color: transparent;\n      color: #fff;\n      font-weight: bold;\n      font-size: 2em;\n    }\n\n    & > div.container {\n      box-sizing: border-box;\n      display: grid;\n      grid-template-columns: 1fr 1fr;\n      column-gap: 3em;\n      padding: 1em;\n      \n      @media screen and (max-width: 991px) {\n        grid-template-columns: 1fr;\n      }\n\n      & > section:first-of-type {\n        @media screen and (max-width: 991px) {\n          margin-bottom: 2em;\n        }\n\n        .video-wrap {\n          position: relative;\n          padding-bottom: 56.25%;\n          height: 0;\n\n          iframe {\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 100%;\n            height: 100%;\n          }\n        }\n      }\n\n      & > section:last-of-type {\n        padding: 2em;\n        background-color: rgba(239, 230, 217, 0.8);\n        text-align: center;\n\n        @media screen and (max-width: 768px) {\n          padding: 1em;\n        }\n\n        h2 {\n          font-size: 1.6em;\n          font-weight: 600;\n        }\n\n        h3 {\n          font-size: 1.3em;\n        }\n\n        hr { \n          background: #fff;\n          opacity: .5;\n          border-color: #fff;\n        }\n\n        p {\n          font-size: 1.3em;\n          max-width: 500px;\n          margin: auto;\n          margin: 1em auto;\n          text-align: left;\n        }\n\n        .benefits {\n          display: grid;\n          grid-template-columns: 1fr 1fr;\n          color: $fdc-teal;\n          padding: 0 1rem;\n\n          @media screen and (max-width: 500px) {\n            grid-template-columns: 1fr;\n          }\n          \n          .fas {\n            font-size: 4em;\n          }\n\n          p {\n            text-align: center;\n          }\n        }\n\n        .fieldset {\n          padding: 1em;\n          border: groove 3px;\n          border-radius: 6px;\n          margin: 0 0 2em;\n\n          @media screen and (max-width: 500px) {\n            text-align: left;\n          }\n\n          .form-check {\n            display: inline-block;\n            margin: 0.5em;\n\n            input {\n              margin-right: 1em;\n            }\n\n            label {\n              vertical-align: text-top;\n            }\n          }\n        }\n\n        .btn.btn-lg {\n          width: 100%;\n          background-color: $fdc-teal;\n          color: #fff;\n          border-color: $fdc-teal;\n        }\n      }\n    }\n  }\n\n  footer {\n    background-color: $fdc-orange;\n    text-align: center;\n    padding: 2em;\n    color: #fff;\n    width: 100%;\n  }\n}"},"lineCount":null}},"error":null,"hash":"0562f24b64cf2395adc80a981c1e89e7","cacheData":{"env":{}}}