 cover;\n    background-repeat: no-repeat;\n    background-position: top right;\n    flex: 1;\n\n    & > div {\n      display: grid;\n      grid-template-columns: 1fr 1fr;\n      column-gap: 3em;\n      padding: 2em 0;\n\n      @media screen and (max-width: 768px) {\n        grid-template-columns: 1fr;\n      }\n\n      & > section:first-of-type {\n        #video {\n          display: inline-block;\n          max-width: 640px;\n          width: 100%;\n          margin-bottom: 2em;\n          border: solid 1em #fff;\n  \n          iframe {\n            width: 100%;\n          }\n        }\n      }\n  \n      & > section:last-of-type {\n        padding: 2em;\n        background-color: rgba(239, 230, 217, 0.8);\n  \n        @media screen and (max-width: 768px) {\n          padding: 1em;\n        }\n  \n        text-align: center;\n  \n        h2 {\n          font-size: 1.6em;\n          font-weight: 600;\n        }\n  \n        h3 {\n          font-size: 1.3em;\n        }\n  \n        hr { \n          background: #fff;\n          opacity: .5;\n          border-color: #fff;\n        }\n  \n        p {\n          font-size: 1.3em;\n          max-width: 500px;\n          margin: auto;\n          margin: 1em auto;\n          text-align: left;\n        } \n  \n        form {\n          input[type=\"text\"] {\n            padding: 6px;\n            margin: 6px;\n          }\n  \n          #sender-info > div,\n          #recipient-info > div {\n            display: grid;\n            grid-template-columns: 1fr 1fr;\n  \n            @media screen and (max-width: 500px) {\n              grid-template-columns: 1fr;\n            }\n          }\n  \n          .submit-btn .btn.btn-lg {\n            margin-top: 1.5em;\n            background-color: $fdc-teal;\n            color: #fff;\n            border-color: $fdc-teal;\n          }\n        }      \n      }\n    }\n  }\n\n  footer {\n    text-align: center;\n    padding: 2em;\n    color: #fff;\n    background-color: $fdc-orange;\n  }\n}"},"lineCount":null}},"error":null,"hash":"09827e451e714f8f22c62823d69c045f","cacheData":{"env":{}}}